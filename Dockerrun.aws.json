{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "graphit/f4e-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "graphit/f4e-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "graphit/f4e-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ],
      "links": ["client", "api", "ssr"],
      "memory": 128
    }
  ]
}
